			<artifactId>spring-boot-starter-web</artifactId>
		</dependency>
		<dependency>
			<groupId>com.fzy</groupId>
			<artifactId>weblog-module-search</artifactId>
		</dependency>
		<!-- 免写冗余的 Java 样板式代码 -->
